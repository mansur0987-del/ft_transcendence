{
    "extends": "airbnb",
    "rules": {
        "indent": [2, 4],
        "no-multi-spaces": [0],
        "key-spacing": [0],
        "no-param-reassign": [2, {"props": false}],
        "no-labels": [2],
        "strict": [0],
        "max-len": [0]
    },
    "env": {
        "node": true
    }
}
